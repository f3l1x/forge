{
  "version": 2,
  "name": "planette-nette-sandbox",
  "alias": ["nette-sandbox.planette.io"],
  "scope": "planette",
  "builds": [
    { "src": "www/index.php", "use": "@juicyfx/php" },
    { "src": "www/checker/index.php", "use": "@juicyfx/php" }
  ],
  "routes": [
    { "src": "/", "dest": "/www/index.php" },
    { "src": "/checker/", "dest": "/www/checker/index.php" }
  ],
  "env": {
    "NOW_ENV": "true"
  }
}